{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "jsx": "react-jsx"
    },
    "include": [
      "tests/**/*.ts",
      "playwright.config.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  